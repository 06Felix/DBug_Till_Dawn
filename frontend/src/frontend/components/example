const transformedData = data.map(post => ({
    reactionCount: post.totalReactionCount,
    likeCount: post.likeCount,
    praiseCount: post.praiseCount,
    commentCount: post.commentsCount,
    empathyCount: post.empathyCount || 0, // Default to 0 if empathyCount is not present
  }));
  console.log(transformedData);
  const options = {
    method: 'GET',
    url: 'https://twitter135.p.rapidapi.com/v2/UserTweetsAndReplies/',
    params: { id: '50883209' }, 
    headers: {
        'x-rapidapi-key': 'e12c0f0426msh4b4df90073d9116p1eb601jsn3e61d27169ca',
        'x-rapidapi-host': 'twitter135.p.rapidapi.com'
    }
};

try {
    const response = await axios.request(options);
    const instructions = response.data.data.user.result.timeline_v2.timeline.instructions;
    console.log(instructions);
    const entries = instructions[instructions.length - 1].entries;

    entries.forEach(x => {
        const tweet = x.content.items;
        if(tweet){
            tweet.forEach(y =>{
                const t = y.item.itemContent.tweet_results;
                if(t && t.result){
                    const views = y.item.itemContent.tweet_results.result.views;
                    if(t.result.legacy){

                        console.log("Favourite Count:", t.result.legacy.favorite_count);
                        console.log("Full Text:", t.result.legacy.full_text);
                        console.log("Reply Count:", t.result.legacy.reply_count);
                        console.log("Retweet Count:", t.result.legacy.retweet_count);
                        console.log("Quote Count:", t.result.legacy.quote_count);
                    }
                    if(views)
                        console.log("Views Count:", views.count);
                    console.log("\n");
                }
            });
        }

    });
} catch (error) {
    console.error(error);
}